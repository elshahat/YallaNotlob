<%include layout/header%>
<!-- #Header Starts -->

<script src="assets/libraries/jquery-3.1.1.js"></script>
<script src="assets/libraries/socket.io.js"></script>
<script src="assets/js/homeSocket.js"></script>

<!-- //////////////////friends Page/////////////////////////// -->
<div id="friends">
    <!-- Slider Starts -->
    <div class="banner">
        <img src="assets/images/back.jpg" alt="banner" class="img-responsive">
        <div class="caption">
            <div class="caption-wrapper">
                <div class="caption-info">

                  <div class="add" style="margin-top:115px">
                    <div>
                      <label for="user" class="heading" style="margin-left: -362px;">Your Friend's Email</label>
                      <input id="friendemail2Add" type="text">
                    </div>
                    <div style="width: 15%;">
                        <button type="submit" id="addFreindBtn" class="btn btn-primary" style="width:49%; margin-left:212px;z-index: 1100;position: relative;">Add</button>
                        <p id="addFriendError" style="font-size: 15px;margin-top: 24px;margin-left: 168px;z-index: 20000;position: relative;width: 276px;"></p>
                    </div>
                  </div>

                  <div class="border-wrap">
                    <div class="friendsListBorder" style="overflow-x: auto;">
                      <h3 style="margin:10px">Friends List</h3>
                      <div class="wrapper row3">
                        <main class="hoc container clear">
                          <div class="sectiontitle">
                          </div>
                          <div class="group excerpts" id="friendsList">
                            <%for (var i=0;i<FrindsEmail.length;i++){%>
                            <article class="one_third" style="margin-top:-75px;">
                              <div class="hgroup">
                                <h6 class="heading" style="color: goldenrod;"><%=FrindsEmail[i]%></h6>
                                <p  style="display:none"><%=FrindsEmail[i]%></p>
                              </div>
                              <img src="../assets/images/1.jpg" alt="">
                              <footer><button id="<%=FrindsEmail[i]%>" type="button" class="btn removeClass" >UnFriend &raquo;</button></footer>
                            </article>
                            <%}%>
                          </div>
                          <div class="clear"></div>
                        </main>
                      </div>
                    </div>
                  </div>
                </div>
                </div>
            </div>
        </div>
    </div>
</div>
    <!--....................................... jquery........................ -->
    <script src="assets/libraries/jquery.js"></script>
    <script src="assets/libraries/wow.min.js"></script>
    <script src="assets/libraries/bootstrap.js" type="text/javascript" ></script>
    <script src="assets/libraries/touchSwipe.min.js"></script>
    <script src="assets/libraries/respond.js"></script>
    <script src="assets/libraries/jquery.blueimp-gallery.min.js"></script>
    <script src="assets/js/script.js"></script>
    <script src="layout/scripts/jquery.min.js"></script>
    <script src="layout/scripts/jquery.backtotop.js"></script>
    <script src="layout/scripts/jquery.mobilemenu.js"></script>
</body>
</html>
