                        <div class="caption-info">
                            <h1 class="animated bounceInUp">Best place for ordering food and drinks</h1>
                            <!--Login/ Sign Up form-->
                            <div class="login-wrap">
                	            <div class="login-html">
                                    <!-- zaynab -->
                                    <% if((signMsg=="trueSignIn")||(signMsg=="falseSignIn")){ %>
                                        <input id="tab-1" type="radio" name="tab" class="sign-in" checked><label for="tab-1" class="tab">Sign In</label>
                                        <input id="tab-2" type="radio" name="tab" class="sign-up"><label for="tab-2" class="tab">Sign Up</label>
                                    <% } else { %>
                                        <input id="tab-1" type="radio" name="tab" class="sign-in"><label for="tab-1" class="tab">Sign In</label>
                                        <input id="tab-2" type="radio" name="tab" class="sign-up" checked><label for="tab-2" class="tab">Sign Up</label>
                                    <% } %>
                                <div class="login-form">
                                    <form class="" action="/signinAuth" method="post">
                			        <div class="sign-in-htm">
                				        <div class="group">
                				            <label for="user" class="label">Email</label>
                			                <input id="user" type="email"class="input logEmail" name="email" required>
                			            </div>
                			            <div class="group">
                			                <label for="pass" class="label">Password</label>
                			                <input id="pass" type="password"class="input" data-type="password" name="password" required>
                			            </div>
                			            <div class="group">
                			                <input type="submit" class="button" id="signInBtn" value="Sign In">
                			            </div>
                			            <div class="hr"></div>
                                  </form>
                			            <div class="foot-lnk">
                                    <form  class="" action="/forgotPass"  method="post" >
                                      <input type="submit" id="forgotPassword" value="forgot Password?">
                                      <!-- <button type="submit" style="background-color:transparent">Forgotgtt Password?</button> -->
                                    </form>
                				        </div>
                                        <!-- zaynab -->
                                        <!-- missing style -->
                                        <div>
                                            <% if(signMsg=="falseSignIn"){ %>
                                                <b>Wrong username or password!!</b>
                                            <% } else { %>
                                                <b></b>
                                            <% } %>
                                        </div>
                                        <div class="socialMedia">
                					        <a href="#facebook"><img src="assets/images/facebook.png" width="60px" height="60px"></a>
                                  <a href="#goole"><img src="assets/images/google.png" width="70px" height="70px"></a>
                				        </div>
                			        </div>
                            <!-- edited -->
                			        <div class="sign-up-htm">
                                        <form class="" action="/signupAuth" method="post">
                				        <div class="group">
                					        <label for="user" class="label">Username</label>
                					        <input id="user" type="text"class="input" name="name" required>
                			            </div>
                                        <div class="group">
                					        <label for="pass" class="label">Email</label>
                					        <input id="pass" type="email"class="input logEmail" name="email" required>
                				        </div>
                		                <div class="group">
            				                <label for="pass" class="label">Password</label>
                					        <input id="pass" type="password"class="input" data-type="password" name="password1" min="5" required>
                				        </div>
                				        <div class="group">
                					        <label for="pass" class="label">Confirm Password</label>
                					        <input id="pass" type="password"class="input" data-type="password" name="password2" required>
                				        </div>
                                        <!-- zaynab -->
                                        <!-- missing style -->
                                        <div>
                                            <% if(signMsg=="falseSignUp-password"){ %>
                                                <b>password Not Matched!!</b>
                                            <% }else if(signMsg=="falseSignUp-Email"){ %>
                                                <b>Email already Exist !!</b>
                                            <% } else { %>
                                                <b></b>
                                            <% } %>
                                        </div>
                				        <div class="group">
                					        <input type="submit" class="button" id="signUpBtn" value="Sign Up">
                				        </div>
                                    </form>
                			        </div>
                		        </div>
                	        </div>
                        </div>
                    </div>
                </div>
