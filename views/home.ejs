<%include layout/header%>


    <script src="assets/libraries/jquery-3.1.1.js"></script>
    <script src="assets/libraries/socket.io.js"></script>
    <script src="assets/js/homeSocket.js"></script>
<!-- //////////////////Home Page/////////////////////////// -->
<div id="home">
    <!-- Slider Starts -->
    <div class="banner">
        <img src="assets/images/back.jpg" alt="banner" class="img-responsive">
        <div class="caption">
            <div class="caption-wrapper">
                <div class="caption-info">
                  <div class="border-wrap" >
                 <div class="latestOrederBorder" style="overflow-y: auto;">
                   <h3 style="margin:10px">Latest Oreders</h3>
                   <ul style="list-style-type:none;margin-left: -44px;margin-top: 35px;">
                       <%for(var i=0;i<Types.length;i++){%>
                           <li><a><%=Types[i]%> on <%=Dates[i]%></a></li>
                           <br>
                        <%}%>
                   </ul>
                </div>
                <div class="friendsActivityBorder" style="width: 504px;overflow-y:auto">
                  <h3 style="margin:5px">Friends Activity</h3>
                  <ul style="list-style-type:none;margin-left: -44px;margin-top:35px;">
                    <%for(var i=0;i<getAct.length;i++){%>
                      <%for(var c=0;c<getAct[i].length;c++){%>
                        <li><a> <%=Friends[i]%> </a> Created an Order from <%=getAct[i][c].from%> for <%=getAct[i][c].type%></li>
                        <br>
                      <%}%>
                    <%}%>
                  </ul>
               </div>
              </div>
                </div>
            </div>
        </div>
    </div>
</div>


    <!--....................................... jquery........................ -->
    <script src="assets/libraries/jquery.js"></script>
    <script src="assets/libraries/wow.min.js"></script>
    <script src="assets/libraries/bootstrap.js" type="text/javascript" ></script>
    <script src="assets/libraries/touchSwipe.min.js"></script>
    <script src="assets/libraries/respond.js"></script>
    <script src="assets/libraries/jquery.blueimp-gallery.min.js"></script>


</body>
</html>
