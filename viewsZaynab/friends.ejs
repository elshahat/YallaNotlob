<%include layout/header%>
<!-- #Header Starts -->
<!-- //////////////////friends Page/////////////////////////// -->
<div id="friends">
    <!-- Slider Starts -->
    <div class="banner">
        <img src="assets/images/back.jpg" alt="banner" class="img-responsive">
        <div class="caption">
            <div class="caption-wrapper">
                <div class="caption-info">

                  <div class="add">
                    <form class="" action="/" method="post">
                    <div>
                      <label for="user" class="heading" style="margin-left: -359px;">Your Friend's Email</label>
                      <input id="user" type="text" style="color: black">
                    </div>
                    <div>
                      <input type="submit" class="btn" value="Add" style="margin-top: 25px;margin-right: 580px;">
                    </div>
                </form>
                </div>


                  <div class="border-wrap">
                    <div class="friendsListBorder">
                      <h3 style="margin:10px">Friends List</h3>
                      <div class="wrapper row3">
                        <main class="hoc container clear">
                          <div class="sectiontitle">
                          </div>
                          <div class="group excerpts">
                            <%for (var i=0;i<FrindsName.length;i++){%>
                            <article class="one_third">
                              <div class="hgroup">
                                <h6 class="heading"><%=FrindsName[i]%></h6>
                                <p  style={display="none"}><%=FrindsEmail[i]%></p>
                              </div>
                              <img src="../assets/images/1.jpg" alt="">
                              <footer><a class="btn" href="/friends/unfriend/<%=FrindsEmail[i]%>">UnFriend &raquo;</a></footer>
                            </article>
                            <%}%>
                          </div>
                          <div class="clear"></div>
                        </main>
                      </div>
                    <!-- </div>
                  </div> -->
